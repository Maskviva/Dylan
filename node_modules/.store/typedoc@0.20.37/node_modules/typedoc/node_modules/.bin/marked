#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/21628/Desktop/Dylan - Rebuild.1/node_modules/.store/marked@2.0.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/21628/Desktop/Dylan - Rebuild.1/node_modules/.store/marked@2.0.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../marked@2.0.7/node_modules/marked/bin/marked" "$@"
else
  exec node  "$basedir/../../../../../marked@2.0.7/node_modules/marked/bin/marked" "$@"
fi
